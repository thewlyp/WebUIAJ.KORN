  <section id="search1"></section>
  <!-- Search Section -->
  <section class="content-section text-center">
    <div class="container col-lg-4 mx-auto">
      <div class="jumbotron col-12">
        <h1 class="display-3">Search</h1>
        <form>
          <div class="col"></div>
          <div class="form-group text-left">
            <label for="">Search</label>
            <input type="text" name="keyword" id="keyword" class="form-control" placeholder="Keyword ex. Cafe, Seafood" aria-describedby="helpId">
          </div>
          <div class="form-group text-left">
            <label for="">Category</label>
            <select class="form-control" name="type" id="type">
              <option value="resturant">Resturant</option>
              <option value="doctor">Doctor</option>
              <option value="beauty_salon">Beauty Salon</option>
              <option value="gas_station">Gas Station</option>
              <option value="atm">ATM</option>
            </select>
          </div>
          <div class="form-group text-left">
            <label for="">Distance</label>
            <select class="form-control" name="radius" id="radius">
              <option value="500">500 m</option>
              <option value="1000">1 km</option>
              <option value="5000">5 km</option>
            </select>
          </div>
          <br>
          <a name="search" id="search" class="btn btn-circle js-scroll-trigger" href="#result" role="button">
            <i class="fa fa-angle-double-down animated"></i>
          </a>
        </form>
      </div>
    </div>
  </section>

  <!-- result Section -->
  <section id="result" class="download-section content-section text-center">
    <div class="container col-lg-8 mx-auto">
      <div class="jumbotron2 col-12">
        <br>
        <br>
        <br>
        <h1>Results</h1>
        <b><br>
          <div id="searchresult" class="col-lg-8 mx-auto"></div>
        </b><br><br>
        <div class="footer"><h12 class="explain text-left">*This content was created for Education in Subject 977-290 Workshop II Web user Interface Development</h12></div>
      </div>
    </div>
  </section>

